# mkinitcpio preset file for the 'linux-grsec-tresor' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-grsec-tresor"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-grsec-tresor.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-grsec-tresor-fallback.img" 
fallback_options="-S autodetect"
